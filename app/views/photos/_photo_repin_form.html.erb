<%= form_tag repin_photos_path ,remote: true do %>
  <%= hidden_field_tag "origin_id" , photo.id %>
  <%= hidden_field_tag "box_id", 1 %>
  <%= hidden_field_tag "name" , photo.name %>
  <%= hidden_field_tag "description" , photo.description %>
  <%= submit_tag "PON" , class: "btn btn-success"%>
<% end %>
